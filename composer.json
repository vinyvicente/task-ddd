        "php": ">=7.4",
        "doctrine/orm": "^2.7"
    },
    "license": "MIT",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Vinicius Vicente",
            "email": "vinyvicente@gmail.com"
        }
    ],
    "minimum-stability": "dev",
